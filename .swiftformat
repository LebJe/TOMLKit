--swiftversion 5.4
--binarygrouping none
--decimalgrouping none
--hexgrouping none
--indent tab
--indentcase true
--nospaceoperators "...,..<"
--octalgrouping none
--self insert
--semicolons never
--stripunusedargs closure-only
--wraparguments before-first
--wrapparameters before-first
--disable trailingClosures, typeSugar
--maxwidth 120
